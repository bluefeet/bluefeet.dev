{
  "name": "bluefeet.dev",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build-resume-json": "yq -p yaml -o json public/resume.yaml > public/resume.json",
    "build-resume-type": "npx json2ts -i public/resume.schema.json -o types/Resume.d.ts",
    "build-resume": "npm run build-resume-json && npm run build-resume-type",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-update-snapshots": "jest --updateSnapshot",
    "test-coverage": "jest --coverage",
    "is-pretty": "prettier --check .",
    "make-pretty": "prettier --write .",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@types/howler": "^2.2.11",
    "@types/markdown-it": "^13.0.7",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.18",
    "date-fns": "^3.3.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.1",
    "howler": "^2.2.4",
    "micromark": "^4.0.0",
    "next": "^14.1.1",
    "postcss": "^8.4.33",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.1.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.3.0",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.11",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "json-schema-to-typescript": "^13.1.2",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.4",
    "ts-node": "^10.9.2"
  }
}
