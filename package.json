{
  "name": "bluefeet.dev",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build-resume-json": "yq . public/resume.yaml > public/resume.json",
    "build-resume-zod": "json-refs resolve public/resume.schema.json | json-schema-to-zod -d 100 | prettier --parser typescript > app/zResume.ts",
    "build-resume": "npm run build-resume-json && npm run build-resume-zod",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-update-snapshots": "jest --updateSnapshot",
    "test-coverage": "jest --coverage",
    "is-pretty": "prettier --check .",
    "make-pretty": "prettier --write .",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@heroicons/react": "^2.1.3",
    "date-fns": "^4.1.0",
    "howler": "^2.2.4",
    "micromark": "^4.0.0",
    "next": "^15.1.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.3.0",
    "@testing-library/react": "^16.2.0",
    "@types/howler": "^2.2.11",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/node": "^22.8.5",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.1.7",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "json-refs": "^3.0.15",
    "json-schema-to-zod": "^2.0.14",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.1.3"
  }
}
