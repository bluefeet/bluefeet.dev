{
  "name": "bluefeet.dev",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build-resume-json": "yq . public/resume.yaml > public/resume.json",
    "build-resume-zod": "json-refs resolve public/resume.schema.json | json-schema-to-zod -d 100 | prettier --parser typescript > app/zResume.ts",
    "build-resume": "npm run build-resume-json && npm run build-resume-zod",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-update-snapshots": "jest --updateSnapshot",
    "test-coverage": "jest --coverage",
    "check-format": "prettier --check .",
    "format": "prettier --write .",
    "check-types": "tsc"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.3",
    "date-fns": "^4.1.0",
    "howler": "^2.2.4",
    "micromark": "^4.0.0",
    "next": "^15.1.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.0.6",
    "@testing-library/jest-dom": "^6.3.0",
    "@testing-library/react": "^16.2.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/howler": "^2.2.11",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/node": "^22.8.5",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "clsx": "^2.1.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.1.7",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "json-refs": "^3.0.15",
    "json-schema-to-zod": "^2.0.14",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.6",
    "typescript": "^5.1.3"
  }
}
